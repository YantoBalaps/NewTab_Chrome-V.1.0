{
  "manifest_version": 3,
  "name": "My New Tab",
  "version": "1.0",
  "description": "New tab with background video, shortcuts, and snow effect",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [],
  "icons": {
    "128": "/images/icon.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "videos/background4.mp4",
        "images/1.png",
        "images/2.png",
        "images/3.png",
        "images/4.png",
        "images/5.png",
        "images/6.png"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}
